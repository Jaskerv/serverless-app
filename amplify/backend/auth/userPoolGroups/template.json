{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Parameters": {
        "authServerlessWebAppUserPoolId": {
            "Type": "String",
            "Default": "authServerlessWebAppUserPoolId"
        },
        
        "authServerlessWebAppAppClientID": {
            "Type": "String",
            "Default": "authServerlessWebAppAppClientID"
        },
        "authServerlessWebAppAppClientIDWeb": {
            "Type": "String",
            "Default": "authServerlessWebAppAppClientIDWeb"
        },
        "AuthRoleArn": {
            "Type": "String"
        },
        "UnauthRoleArn": {
            "Type": "String"
        },
        "env": {
            "Type": "String"
        }
    },
    "Conditions": {
        "ShouldNotCreateEnvResources": {
            "Fn::Equals": [
                {
                    "Ref": "env"
                },
                "NONE"
            ]
        }
    },
    "Resources": {
    
        "freeGroup": {
          "Type" : "AWS::Cognito::UserPoolGroup",
          "Properties" : {
              "GroupName" : "free",
              "Precedence" : 1,
              
              "UserPoolId" : {
                "Ref": "authServerlessWebAppUserPoolId"
              } 
            }
        },
        
    
    
    },
    
}
